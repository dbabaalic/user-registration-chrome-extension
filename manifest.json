{
    "manifest_version": 2,
    "name": "User registration app",
    "description":"Simple registration app for chrome",
    "version":"1.0",
    "browser_action": {
        "default_icon":"icon.png",
        "default_popup":"index.html"
    },  
    "content_scripts": [{
     "js": ["lib/angular.min.js"],
     "matches": ["http://*/*"]
   }],
  "permissions": [
    "debugger",
    "storage"
  ]
}
